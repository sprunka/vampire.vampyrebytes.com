{
    "name": "sprunka/vampire-api",
    "description": "This is an API for use with Fifth Edition Vampire: the Masquerade",
    "minimum-stability": "dev",
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "Sean Prunka",
            "email": "sprunka@gmail.com"
        },
        {
            "name": "Vampyre Bytes",
            "email": "admin@vampyrebytes.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-calendar": "*",
        "slim/slim": "^4.5",
        "slim/psr7": "^1.2",
        "php-di/php-di": "dev-master",
        "selective/basepath": "dev-master",
        "zircote/swagger-php": "^3.1",
        "fakerphp/faker": "^1.15"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9.4",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "VampireAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VampireAPI\\Test\\": "tests/"
        }
    }
}
